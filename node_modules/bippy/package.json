{
  "name": "bippy",
  "version": "0.5.16",
  "description": "hack into react internals",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "bippy",
    "react",
    "react instrumentation",
    "react devtools",
    "react fiber",
    "fiber",
    "internals"
  ],
  "homepage": "https://bippy.dev",
  "bugs": {
    "url": "https://github.com/aidenybai/bippy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aidenybai/bippy.git"
  },
  "license": "MIT",
  "author": {
    "name": "Aiden Bai",
    "email": "aiden@million.dev"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./core": {
      "import": {
        "types": "./dist/core.d.ts",
        "default": "./dist/core.js"
      },
      "require": {
        "types": "./dist/core.d.cts",
        "default": "./dist/core.cjs"
      }
    },
    "./install-hook-only": {
      "import": {
        "types": "./dist/install-hook-only.d.ts",
        "default": "./dist/install-hook-only.js"
      },
      "require": {
        "types": "./dist/install-hook-only.d.cts",
        "default": "./dist/install-hook-only.cjs"
      }
    },
    "./source": {
      "import": {
        "types": "./dist/source.d.ts",
        "default": "./dist/source.js"
      },
      "require": {
        "types": "./dist/source.d.cts",
        "default": "./dist/source.cjs"
      }
    },
    "./dist/*": "./dist/*.js",
    "./dist/*.js": "./dist/*.js",
    "./dist/*.cjs": "./dist/*.cjs"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "browser": "dist/index.iife.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "bin",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^20",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "@vitest/coverage-istanbul": "2.1.8",
    "esbuild": "^0.24.2",
    "happy-dom": "^15.11.7",
    "publint": "^0.2.12",
    "react": "19.0.0",
    "react-devtools-inline": "^6.0.1",
    "react-dom": "19.0.0",
    "react-reconciler": "^0.31.0",
    "react-refresh": "^0.16.0",
    "terser": "^5.36.0",
    "tsdown": "^0.12.3",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "react": ">=17.0.1"
  },
  "dependencies": {
    "@types/react-reconciler": "^0.28.9"
  },
  "scripts": {
    "build": "NODE_ENV=production tsdown && bun scripts/append-banner.ts",
    "dev": "NODE_ENV=development tsdown --watch",
    "publint": "publint",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  }
}